    <script>
        $.widget.bridge('uibutton', $.ui.button);
    </script>
    <script src="/js/jquery.tokeninput.js"></script>

    <!-- Bootstrap 3.3.7 -->
    <script src="/js/bootstrap.min.js"></script>
    <!-- Morris.js charts -->
    <script src="/js/raphael.min.js"></script>

    <script src="/js/morris.min.js"></script>
